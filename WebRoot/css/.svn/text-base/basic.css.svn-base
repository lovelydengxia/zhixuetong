@charset "utf-8";
/* CSS Document */
/*浮动*/
.fl{float:left;}
.fr{float:right!important;}
/*清除浮动*/
.clr{clear:both;}
/*居中*/
.mauto{margin:0 auto;}
.mauto20{margin:20px auto;}
.tc{text-align:center;}
.tr{text-align:right;}
.tl{ text-align:left!important;}
/*字体*/
.f12{font-size:12px;}
/*行高*/
.lh15{line-height:15px!important;}
.lh30{line-height:30px!important;}
/*边距*/
.ml5{ margin-left:5px;}
.ml10{margin-left:10px;}
.ml20{margin-left:20px;}
.ml30{margin-left:30px;}
.ml40{ margin-left:40px;}
.ml50{margin-left:50px!important;}
.ml60{margin-left:60px;}
.ml100{margin-left:100px;}
.ml110{margin-left:110px;}
.ml130{margin-left:130px;}
.mr5{margin-right:5px;}
.mr10{margin-right:10px;}
.mr20{margin-right:20px;}
.mr40{ margin-right:40px;}
.mr60{margin-right:60px;}
.mt5{margin-top:5px;}
.mt10{margin-top:10px;}
.mt20{margin-top:20px!important;}
.mt30{ margin-top:30px;}
.mt50{margin-top:50px!important;}
.mt100{margin-top:100px!important;}
.mb10{margin-bottom:10px!important;}
.mb20{margin-bottom:20px!important;}
.mb50{margin-bottom:50px;}
.mb0{ margin-bottom:0!important;}
.pt30{padding-top:30px;}
.pt20{padding-top:20px;}
.pl10{padding-left:10px;}
.pt5{ padding-top:5px;}
/*宽度*/
.w20{width:20px;}
/*color*/
.red{color:#e60012;}
.c999{color:#999;}
.c666{ color:#666!important;}
.disnone{display:none;}
.ce70012{ color:#e70012!important;}
.c4d9ac6{ color:#4d9ac6!important;}
.c11c036{ color:#11c036!important;}
.c03aefd{ color:#03aefd!important;}
.c248ff2{ color:#248ff2!important;}
/*字体*/
.fw{ font-weight:bold;}
.f18{ font-size:18px;}
.f22{ font-size:22px;}

/*button*/
.modifyBot{ width:90px; height:30px; background:#fb9004; color:#fff; font-family:"微软雅黑"; border-radius:3px;}
label.liLabel{ display:inline-block; width:90px; margin-right:10px; text-align:right;}
/*page*/
.page{ width:670px; margin:0 auto 60px;}
.pageL a{ float:left; padding:0 12px; height:35px; margin-right:7px; text-align: center; line-height:35px; border:1px solid #eeeeee; color:#999999;}
.pageL a:hover{ color:#fd6500; border:1px solid #fd6500;}
.pageR{ margin-left:20px; line-height:35px; color:#999;}
.pageR input[type=text]{ width:44px; height:20px; border:1px solid #eeeeee; font-size:12px; color:#999; margin:0 10px;}
.pageR input[type=button]{ width:44px; height:25px; background:#fff; border:1px solid #eee; color:#5b5b5b; font-size:12px; cursor:pointer;}


